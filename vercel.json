{
  "version": 2,
  "buildCommand": "npm run setup && npm run build",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/build/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
